generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql" // Changed!
  url      = env("DATABASE_URL")
}

model Asset {
  id        String   @id @default(cuid())
  title     String
  type      String
  url       String
  width     Int
  height    Int
  size      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
